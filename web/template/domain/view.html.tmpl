{{define "content"}}
<div class="detail-page">
	<h1>{{.Title}}</h1>
	{{if .CurrentUser.HasPrivilege "domains.update"}}
		<a class="button-link" href="/admin/domain/update/{{.Domain.ID}}">Edit Domain</a>
	{{end}}
	<div>
		<p>ID: {{.Domain.ID}}</p>
		<p>Name: {{.Domain.Name}}</p>
		<p>Created at: {{.Domain.CreatedAt}}</p>
		<p>Updated at: {{.Domain.UpdatedAt}}</p>
	</div>
</div>
{{end}}
