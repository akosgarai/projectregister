{{define "content"}}
<div class="detail-page">
	<h1>{{.Title}}</h1>
	<form action="/admin/application/import-to-environment/{{.Environment.ID}}" method="post" enctype="multipart/form-data">
		<input type="hidden" name="environment_id" value="{{.Environment.ID}}"/>
		<div class="form-group">
			<label for="csvfile">CSV file</label>
			<input type="file" class="form-control" id="csvfile" name="csvfile" >
		</div>
		<button type="submit" class="btn btn-primary">Upload</button>
	</form>
</div>
{{end}}
