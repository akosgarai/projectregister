{{define "content"}}
<div class="listing-page">
	{{template "header" .Header}}
	<table>
		{{range $rowIndex, $item  := .Result}}
			<tr>
				<td>{{$rowIndex}}</td>
				{{range $i, $col := $item.RowData}}
					<td>{{$col}}</td>
				{{end}}
				<td>{{$item.ErrorMessage}}</td>
				<td>{{if eq $item.ErrorMessage ""}}{{$item.Application.ID}}{{end}}</td>
			</tr>
		{{end}}
	</table>
</div>
{{end}}
