{{define "content"}}
<div class="detail-page">
	<h1>{{.Title}}</h1>
	{{if .CurrentUser.HasPrivilege "runtimes.update"}}
		<a class="button-link" href="/admin/runtime/update/{{.Runtime.ID}}">Edit Runtime</a>
	{{end}}
	<div>
		<p>ID: {{.Runtime.ID}}</p>
		<p>Name: {{.Runtime.Name}}</p>
		<p>Created at: {{.Runtime.CreatedAt}}</p>
		<p>Updated at: {{.Runtime.UpdatedAt}}</p>
	</div>
</div>
{{end}}
